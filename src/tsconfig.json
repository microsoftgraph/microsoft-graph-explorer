{
  "compilerOptions": {
    "module": "commonjs",
    "noEmitOnError": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "removeComments": true,
    "lib": [ "es2015", "dom" ],
    "sourceMap": true,
    "target": "es5",
    "declaration": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  }
}